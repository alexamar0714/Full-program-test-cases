<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
    <!-- For Java 7:
    <web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
             version="3.1">
     -->
    <display-name>iws-ws</display-name>

    <servlet>
        <servlet-name>accessWS</servlet-name>
        <servlet-class>net.iaeste.iws.ws.AccessWS</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>accessWS</servlet-name>
        <url-pattern>/accessWS</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>administrationWS</servlet-name>
        <servlet-class>net.iaeste.iws.ws.AdministrationWS</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>administrationWS</servlet-name>
        <url-pattern>/administrationWS</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>committeeWS</servlet-name>
        <servlet-class>net.iaeste.iws.ws.CommitteeWS</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>committeeWS</servlet-name>
        <url-pattern>/committeeWS</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>exchangeWS</servlet-name>
        <servlet-class>net.iaeste.iws.ws.ExchangeWS</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>exchangeWS</servlet-name>
        <url-pattern>/exchangeWS</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>storageWS</servlet-name>
        <servlet-class>net.iaeste.iws.ws.StorageWS</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>storageWS</servlet-name>
        <url-pattern>/storageWS</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>studentWS</servlet-name>
        <servlet-class>net.iaeste.iws.ws.StudentWS</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>studentWS</servlet-name>
        <url-pattern>/studentWS</url-pattern>
    </servlet-mapping>
</web-app>
